{
    "name": "trismegiste/dokudoki",
    "description": "Schemaless database layer for MongoDB and helpers for schemaless capabilities",
    "type": "symfony-bundle",
    "keywords": ["dbal", "mongodb", "symfony2"],
    "authors": [
        {
            "name": "Trismegiste"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-mongo": ">=1.3.0",
        "trismegiste/yuurei": "2.2.*"
    },
    "require-dev": {
        "symfony/http-kernel": "2.3.*",
        "symfony/dependency-injection": "2.3.*",
        "symfony/yaml": "2.3.*",
        "symfony/config": "2.3.*",
        "symfony/form": "2.3.*",
        "symfony/console": "2.3.*",
        "nikic/php-parser": "0.9.x-dev"
    },
    "autoload": {
        "psr-0": {
            "Trismegiste\\DokudokiBundle\\": "src"
        }
    },
    "license": "MIT",
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
}
