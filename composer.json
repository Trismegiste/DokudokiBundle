{
    "name": "trismegiste/dokudoki",
    "description": "Schemaless database layer for MongoDB and helpers for schemaless capabilities",
    "type": "symfony-bundle",
    "keywords": ["dbal", "mongodb", "symfony2"],
    "authors": [
    {
        "name": "Trismegiste",
        "email": "florent.g@allopneus.com"
    }
    ],
   "require": {
        "php": ">=5.4",
        "ext-mongo": ">=1.3.0"
    },
    "require-dev": {
        "symfony/http-kernel": "2.2.*",
        "symfony/dependency-injection": "2.2.*",
        "symfony/yaml": "2.2.*",
        "symfony/config": "2.2.*",
        "symfony/form": "2.2.*",
        "symfony/console": "2.2.*",
        "nikic/php-parser": "0.9.x-dev"
    },
    "autoload": {
        "psr-0": {
            "Trismegiste\\DokudokiBundle": ""
        }
